# tool: scribe-data
# All latin (Q397) proper_nouns (Q147276) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?proper_nouns
    ?accusative
  ?singularNominativeMasculine
  ?singularVocative
  ?singularAblativeMasculine
  ?accusativePluralMasculine
  ?genitivePlural
  ?dativePlural
  ?nominative
  ?pluralVocative
  ?singularLocative
  ?nominativePluralMasculine
  ?vocative
  ?singularDativeMasculine
  ?masculine
  ?feminine
  ?pluralVocativeMasculine
  ?pluralLocativeMasculine

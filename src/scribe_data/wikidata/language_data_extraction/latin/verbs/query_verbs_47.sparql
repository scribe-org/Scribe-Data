# tool: scribe-data
# All Latin (Q397) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?nominativeNeuterPresentPluralParticiple
  ?nominativeNeuterFutureSingularParticiple
  ?nominativeNeuterFuturePluralParticiple
  ?nominativeNeuterActivePresentPluralParticiple
  ?nominativeNeuterActiveFutureSingularParticiple
  ?nominativeNeuterActiveFuturePluralParticiple

WHERE {
  ?lexeme dct:language wd:Q397 ;
      wikibase:lexicalCategory wd:Q24905 ;
      wikibase:lemma ?verb ;
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeNeuterPresentPluralParticipleForm .
    ?nominativeNeuterPresentPluralParticipleForm ontolex:representation ?nominativeNeuterPresentPluralParticiple ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q1775461, wd:Q192613, wd:Q146786, wd:Q814722 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeNeuterFutureSingularParticipleForm .
    ?nominativeNeuterFutureSingularParticipleForm ontolex:representation ?nominativeNeuterFutureSingularParticiple ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q1775461, wd:Q501405, wd:Q110786, wd:Q814722 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeNeuterFuturePluralParticipleForm .
    ?nominativeNeuterFuturePluralParticipleForm ontolex:representation ?nominativeNeuterFuturePluralParticiple ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q1775461, wd:Q501405, wd:Q146786, wd:Q814722 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeNeuterActivePresentPluralParticipleForm .
    ?nominativeNeuterActivePresentPluralParticipleForm ontolex:representation ?nominativeNeuterActivePresentPluralParticiple ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q1775461, wd:Q1317831, wd:Q192613, wd:Q146786, wd:Q814722 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeNeuterActiveFutureSingularParticipleForm .
    ?nominativeNeuterActiveFutureSingularParticipleForm ontolex:representation ?nominativeNeuterActiveFutureSingularParticiple ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q1775461, wd:Q1317831, wd:Q501405, wd:Q110786, wd:Q814722 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeNeuterActiveFuturePluralParticipleForm .
    ?nominativeNeuterActiveFuturePluralParticipleForm ontolex:representation ?nominativeNeuterActiveFuturePluralParticiple ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q1775461, wd:Q1317831, wd:Q501405, wd:Q146786, wd:Q814722 .
  }
}

# tool: scribe-data
# All Latin (Q397) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?indicativePassivePresentFirstPersonSingular
  ?indicativePassivePresentFirstPersonPlural
  ?indicativePassivePresentSecondPersonPlural
  ?indicativePassivePresentThirdPersonSingular
  ?indicativePassivePresentThirdPersonPlural
  ?indicativePassivePastImperfectFirstPersonSingular

WHERE {
  ?lexeme dct:language wd:Q397 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentFirstPersonSingularForm .
    ?indicativePassivePresentFirstPersonSingularForm ontolex:representation ?indicativePassivePresentFirstPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q21714344, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentFirstPersonPluralForm .
    ?indicativePassivePresentFirstPersonPluralForm ontolex:representation ?indicativePassivePresentFirstPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q21714344, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentSecondPersonPluralForm .
    ?indicativePassivePresentSecondPersonPluralForm ontolex:representation ?indicativePassivePresentSecondPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929049, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentThirdPersonSingularForm .
    ?indicativePassivePresentThirdPersonSingularForm ontolex:representation ?indicativePassivePresentThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929074, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentThirdPersonPluralForm .
    ?indicativePassivePresentThirdPersonPluralForm ontolex:representation ?indicativePassivePresentThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePastImperfectFirstPersonSingularForm .
    ?indicativePassivePastImperfectFirstPersonSingularForm ontolex:representation ?indicativePassivePastImperfectFirstPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q12547192, wd:Q21714344, wd:Q110786 .
  }
}

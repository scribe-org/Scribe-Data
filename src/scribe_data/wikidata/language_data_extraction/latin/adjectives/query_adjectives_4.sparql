# tool: scribe-data
# All Latin (Q397) adjectives (Q34698) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?adjective
  ?accusativePluralFemininePositive
  ?accusativePluralMasculinePositive
  ?accusativePluralNeuterPositive
  ?singularAblativeFemininePositive
  ?singularAblativeMasculinePositive
  ?singularAblativeNeuterPositive

WHERE {
  ?lexeme dct:language wd:Q397 ;
    wikibase:lexicalCategory wd:Q34698 ;
    wikibase:lemma ?adjective ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?accusativePluralFemininePositiveForm .
    ?accusativePluralFemininePositiveForm ontolex:representation ?accusativePluralFemininePositive ;
      wikibase:grammaticalFeature wd:Q146078, wd:Q146786, wd:Q1775415, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?accusativePluralMasculinePositiveForm .
    ?accusativePluralMasculinePositiveForm ontolex:representation ?accusativePluralMasculinePositive ;
      wikibase:grammaticalFeature wd:Q146078, wd:Q146786, wd:Q499327, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?accusativePluralNeuterPositiveForm .
    ?accusativePluralNeuterPositiveForm ontolex:representation ?accusativePluralNeuterPositive ;
      wikibase:grammaticalFeature wd:Q146078, wd:Q146786, wd:Q1775461, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularAblativeFemininePositiveForm .
    ?singularAblativeFemininePositiveForm ontolex:representation ?singularAblativeFemininePositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q156986, wd:Q1775415, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularAblativeMasculinePositiveForm .
    ?singularAblativeMasculinePositiveForm ontolex:representation ?singularAblativeMasculinePositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q156986, wd:Q499327, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularAblativeNeuterPositiveForm .
    ?singularAblativeNeuterPositiveForm ontolex:representation ?singularAblativeNeuterPositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q156986, wd:Q1775461, wd:Q3482678 .
  }
}

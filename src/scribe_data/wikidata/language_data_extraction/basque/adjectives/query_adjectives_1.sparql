# tool: scribe-data
# All basque (Q8752) adjectives (Q34698) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?adjectives
    ?pluralInessiveInanimate
  ?singularAblativeAnimate
  ?genitivePlural
  ?singularInessiveAnimate
  ?singularDative
  ?singularInessiveInanimate
  ?pluralLocativeInanimate
  ?singularInstrumental
  ?singularGenitive
  ?pluralAbsolutive
  ?comparative
  ?absolutiveIndefinite
  ?pluralInessiveAnimate
  ?singularLocativeInanimate
  ?superlative
  ?dativePlural
  ?singularInanimateAllative
  ?singularAblativeInanimate
  ?singularAbsolutive
  ?pluralAnimateAllative
  ?pluralInstrumental
  ?pluralAblativeAnimate
  ?pluralInanimateAllative
  ?pluralAblativeInanimate
  ?singularAnimateAllative

WHERE {
  ?lexeme dct:language wd:Q8752 ;
  wikibase:lexicalCategory wd:Q34698 ;
  wikibase:lemma ?adjectives .
  FILTER(lang(?adjectives) = "eu")
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralInessiveInanimateForm .
    ?pluralInessiveInanimateForm ontolex:representation ?pluralInessiveInanimate ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q282031, wd:Q51927539 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularAblativeAnimateForm .

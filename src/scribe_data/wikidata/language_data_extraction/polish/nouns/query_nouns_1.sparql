      wikibase:grammaticalFeature wd:Q131105, wd:Q146078, wd:Q146786, wd:Q185077 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularDativeLocativeForm .
    ?singularDativeLocativeForm ontolex:representation ?singularDativeLocative ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q145599, wd:Q202142 .
  }

  OPTIONAL {

# tool: scribe-data
# All Polish (Q809) adjectives (Q34698) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?adjective
  ?nominativePluralPositiveMasculinePersonal
  ?nominativePluralPositiveNotMasculinePersonal
  ?singularGenitiveFemininePositive
  ?singularInstrumentalFemininePositive
  ?singularGenitiveMasculineNeuterPositive
  ?singularInstrumentalMasculineNeuterPositive

WHERE {
  ?lexeme dct:language wd:Q809 ;
    wikibase:lexicalCategory wd:Q34698 ;
    wikibase:lemma ?adjective ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativePluralPositiveMasculinePersonalForm .
    ?nominativePluralPositiveMasculinePersonalForm ontolex:representation ?nominativePluralPositiveMasculinePersonal ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q146786, wd:Q3482678, wd:Q27918551 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativePluralPositiveNotMasculinePersonalForm .
    ?nominativePluralPositiveNotMasculinePersonalForm ontolex:representation ?nominativePluralPositiveNotMasculinePersonal ;
      wikibase:grammaticalFeature wd:Q131105, wd:Q146786, wd:Q3482678, wd:Q54152717 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularGenitiveFemininePositiveForm .
    ?singularGenitiveFemininePositiveForm ontolex:representation ?singularGenitiveFemininePositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q146233, wd:Q1775415, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularInstrumentalFemininePositiveForm .
    ?singularInstrumentalFemininePositiveForm ontolex:representation ?singularInstrumentalFemininePositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q192997, wd:Q1775415, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularGenitiveMasculineNeuterPositiveForm .
    ?singularGenitiveMasculineNeuterPositiveForm ontolex:representation ?singularGenitiveMasculineNeuterPositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q146233, wd:Q499327, wd:Q1775461, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularInstrumentalMasculineNeuterPositiveForm .
    ?singularInstrumentalMasculineNeuterPositiveForm ontolex:representation ?singularInstrumentalMasculineNeuterPositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q192997, wd:Q499327, wd:Q1775461, wd:Q3482678 .
  }
}

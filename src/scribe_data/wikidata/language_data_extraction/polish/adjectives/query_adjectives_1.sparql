# tool: scribe-data
# All Polish (Q809) adjectives (Q34698) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?adjective
  ?singularDativeFemininePositive
  ?singularLocativeFemininePositive
  ?genitivePluralPositiveMasculinePersonalNotMasculinePersonal
  ?singularDativeMasculineNeuterPositive
  ?dativePluralPositiveMasculinePersonalNotMasculinePersonal
  ?singularLocativeMasculineNeuterPositive

WHERE {
  ?lexeme dct:language wd:Q809 ;
    wikibase:lexicalCategory wd:Q34698 ;
    wikibase:lemma ?adjective ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularDativeFemininePositiveForm .
    ?singularDativeFemininePositiveForm ontolex:representation ?singularDativeFemininePositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q145599, wd:Q1775415, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularLocativeFemininePositiveForm .
    ?singularLocativeFemininePositiveForm ontolex:representation ?singularLocativeFemininePositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q202142, wd:Q1775415, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?genitivePluralPositiveMasculinePersonalNotMasculinePersonalForm .
    ?genitivePluralPositiveMasculinePersonalNotMasculinePersonalForm ontolex:representation ?genitivePluralPositiveMasculinePersonalNotMasculinePersonal ;
      wikibase:grammaticalFeature wd:Q146233, wd:Q146786, wd:Q3482678, wd:Q27918551, wd:Q54152717 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularDativeMasculineNeuterPositiveForm .
    ?singularDativeMasculineNeuterPositiveForm ontolex:representation ?singularDativeMasculineNeuterPositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q145599, wd:Q499327, wd:Q1775461, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?dativePluralPositiveMasculinePersonalNotMasculinePersonalForm .
    ?dativePluralPositiveMasculinePersonalNotMasculinePersonalForm ontolex:representation ?dativePluralPositiveMasculinePersonalNotMasculinePersonal ;
      wikibase:grammaticalFeature wd:Q145599, wd:Q146786, wd:Q3482678, wd:Q27918551, wd:Q54152717 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularLocativeMasculineNeuterPositiveForm .
    ?singularLocativeMasculineNeuterPositiveForm ontolex:representation ?singularLocativeMasculineNeuterPositive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q202142, wd:Q499327, wd:Q1775461, wd:Q3482678 .
  }
}

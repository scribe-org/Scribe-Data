# tool: scribe-data
# All Polish (Q809) adjectives (Q34698) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?adjective
  ?genitiveMasculinePersonalNotMasculinePersonalPlural
  ?genitiveMasculinePersonalPluralPositive
  ?genitiveMasculinePersonalNotMasculinePersonalPluralComparative
  ?genitiveMasculinePersonalNotMasculinePersonalPluralSuperlative
  ?genitiveMasculinePersonalNotMasculinePersonalPluralPositive

WHERE {
  ?lexeme dct:language wd:Q809 ;
      wikibase:lexicalCategory wd:Q34698 ;
      wikibase:lemma ?adjective ;
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?genitiveMasculinePersonalNotMasculinePersonalPluralForm .
    ?genitiveMasculinePersonalNotMasculinePersonalPluralForm ontolex:representation ?genitiveMasculinePersonalNotMasculinePersonalPlural ;
      wikibase:grammaticalFeature wd:Q146233, wd:Q27918551, wd:Q54152717, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?genitiveMasculinePersonalPluralPositiveForm .
    ?genitiveMasculinePersonalPluralPositiveForm ontolex:representation ?genitiveMasculinePersonalPluralPositive ;
      wikibase:grammaticalFeature wd:Q146233, wd:Q27918551, wd:Q146786, wd:Q3482678 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?genitiveMasculinePersonalNotMasculinePersonalPluralComparativeForm .
    ?genitiveMasculinePersonalNotMasculinePersonalPluralComparativeForm ontolex:representation ?genitiveMasculinePersonalNotMasculinePersonalPluralComparative ;
      wikibase:grammaticalFeature wd:Q146233, wd:Q27918551, wd:Q54152717, wd:Q146786, wd:Q14169499 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?genitiveMasculinePersonalNotMasculinePersonalPluralSuperlativeForm .
    ?genitiveMasculinePersonalNotMasculinePersonalPluralSuperlativeForm ontolex:representation ?genitiveMasculinePersonalNotMasculinePersonalPluralSuperlative ;
      wikibase:grammaticalFeature wd:Q146233, wd:Q27918551, wd:Q54152717, wd:Q146786, wd:Q1817208 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?genitiveMasculinePersonalNotMasculinePersonalPluralPositiveForm .
    ?genitiveMasculinePersonalNotMasculinePersonalPluralPositiveForm ontolex:representation ?genitiveMasculinePersonalNotMasculinePersonalPluralPositive ;
      wikibase:grammaticalFeature wd:Q146233, wd:Q27918551, wd:Q54152717, wd:Q146786, wd:Q3482678 .
  }
}

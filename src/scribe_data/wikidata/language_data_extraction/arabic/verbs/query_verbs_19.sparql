# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?masculineActivePastDual
  ?masculineActivePerformativeSecondPersonSingular
  ?masculineActivePerformativeThirdPersonDual
  ?masculineActivePastThirdPersonPlural
  ?masculineActiveThirdPersonSingularSubjunctive
  ?masculineActiveThirdPersonPluralSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePastDualForm .
    ?masculineActivePastDualForm ontolex:representation ?masculineActivePastDual ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q1994301, wd:Q110022 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePerformativeSecondPersonSingularForm .
    ?masculineActivePerformativeSecondPersonSingularForm ontolex:representation ?masculineActivePerformativeSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q124351233, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePerformativeThirdPersonDualForm .
    ?masculineActivePerformativeThirdPersonDualForm ontolex:representation ?masculineActivePerformativeThirdPersonDual ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q124351233, wd:Q51929074, wd:Q110022 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePastThirdPersonPluralForm .
    ?masculineActivePastThirdPersonPluralForm ontolex:representation ?masculineActivePastThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q1994301, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActiveThirdPersonSingularSubjunctiveForm .
    ?masculineActiveThirdPersonSingularSubjunctiveForm ontolex:representation ?masculineActiveThirdPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q51929074, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActiveThirdPersonPluralSubjunctiveForm .
    ?masculineActiveThirdPersonPluralSubjunctiveForm ontolex:representation ?masculineActiveThirdPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q51929074, wd:Q146786, wd:Q473746 .
  }
}

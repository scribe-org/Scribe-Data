# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?masculinePassivePresentThirdPersonPlural
  ?masculinePassiveSecondPersonSingularSubjunctive
  ?masculinePassiveThirdPersonPluralSubjunctive
  ?indicativePassivePresentFirstPersonSingular
  ?indicativePassivePresentFirstPersonPlural
  ?indicativePassivePresentSecondPersonDual

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassivePresentThirdPersonPluralForm .
    ?masculinePassivePresentThirdPersonPluralForm ontolex:representation ?masculinePassivePresentThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q192613, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveSecondPersonSingularSubjunctiveForm .
    ?masculinePassiveSecondPersonSingularSubjunctiveForm ontolex:representation ?masculinePassiveSecondPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929049, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveThirdPersonPluralSubjunctiveForm .
    ?masculinePassiveThirdPersonPluralSubjunctiveForm ontolex:representation ?masculinePassiveThirdPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929074, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentFirstPersonSingularForm .
    ?indicativePassivePresentFirstPersonSingularForm ontolex:representation ?indicativePassivePresentFirstPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q21714344, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentFirstPersonPluralForm .
    ?indicativePassivePresentFirstPersonPluralForm ontolex:representation ?indicativePassivePresentFirstPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q21714344, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativePassivePresentSecondPersonDualForm .
    ?indicativePassivePresentSecondPersonDualForm ontolex:representation ?indicativePassivePresentSecondPersonDual ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929049, wd:Q110022 .
  }
}

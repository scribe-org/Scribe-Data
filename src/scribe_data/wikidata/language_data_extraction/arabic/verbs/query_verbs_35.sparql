# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?activePresentFirstPersonSingular
  ?activePresentSecondPersonDual
  ?activePresentFirstPersonSingularSubjunctive
  ?activePresentFirstPersonPluralSubjunctive
  ?activePresentSecondPersonPluralSubjunctive
  ?activePresentSecondPersonDualSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?activePresentFirstPersonSingularForm .
    ?activePresentFirstPersonSingularForm ontolex:representation ?activePresentFirstPersonSingular ;
      wikibase:grammaticalFeature wd:Q1317831, wd:Q192613, wd:Q21714344, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?activePresentSecondPersonDualForm .
    ?activePresentSecondPersonDualForm ontolex:representation ?activePresentSecondPersonDual ;
      wikibase:grammaticalFeature wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q110022 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?activePresentFirstPersonSingularSubjunctiveForm .
    ?activePresentFirstPersonSingularSubjunctiveForm ontolex:representation ?activePresentFirstPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q1317831, wd:Q192613, wd:Q21714344, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?activePresentFirstPersonPluralSubjunctiveForm .
    ?activePresentFirstPersonPluralSubjunctiveForm ontolex:representation ?activePresentFirstPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q1317831, wd:Q192613, wd:Q21714344, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?activePresentSecondPersonPluralSubjunctiveForm .
    ?activePresentSecondPersonPluralSubjunctiveForm ontolex:representation ?activePresentSecondPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?activePresentSecondPersonDualSubjunctiveForm .
    ?activePresentSecondPersonDualSubjunctiveForm ontolex:representation ?activePresentSecondPersonDualSubjunctive ;
      wikibase:grammaticalFeature wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q110022, wd:Q473746 .
  }
}

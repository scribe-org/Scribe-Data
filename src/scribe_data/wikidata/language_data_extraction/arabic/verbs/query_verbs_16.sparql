# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?femininePassivePerformativeSecondPersonSingular
  ?femininePassiveSecondPersonSingularSubjunctive
  ?femininePassiveSecondPersonPluralSubjunctive
  ?femininePassiveThirdPersonSingularSubjunctive
  ?femininePassiveThirdPersonPluralSubjunctive
  ?femininePassiveThirdPersonDualSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?femininePassivePerformativeSecondPersonSingularForm .
    ?femininePassivePerformativeSecondPersonSingularForm ontolex:representation ?femininePassivePerformativeSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1194697, wd:Q124351233, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?femininePassiveSecondPersonSingularSubjunctiveForm .
    ?femininePassiveSecondPersonSingularSubjunctiveForm ontolex:representation ?femininePassiveSecondPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1194697, wd:Q51929049, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?femininePassiveSecondPersonPluralSubjunctiveForm .
    ?femininePassiveSecondPersonPluralSubjunctiveForm ontolex:representation ?femininePassiveSecondPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1194697, wd:Q51929049, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?femininePassiveThirdPersonSingularSubjunctiveForm .
    ?femininePassiveThirdPersonSingularSubjunctiveForm ontolex:representation ?femininePassiveThirdPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1194697, wd:Q51929074, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?femininePassiveThirdPersonPluralSubjunctiveForm .
    ?femininePassiveThirdPersonPluralSubjunctiveForm ontolex:representation ?femininePassiveThirdPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1194697, wd:Q51929074, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?femininePassiveThirdPersonDualSubjunctiveForm .
    ?femininePassiveThirdPersonDualSubjunctiveForm ontolex:representation ?femininePassiveThirdPersonDualSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1194697, wd:Q51929074, wd:Q110022, wd:Q473746 .
  }
}

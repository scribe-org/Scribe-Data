# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?feminineActivePastPerfectSecondPersonPlural
  ?feminineActiveThirdPersonSingularSubjunctive
  ?feminineActiveThirdPersonDualSubjunctive
  ?feminineActivePresentThirdPersonPluralSubjunctive
  ?feminineActivePresentThirdPersonDualSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePastPerfectSecondPersonPluralForm .
    ?feminineActivePastPerfectSecondPersonPluralForm ontolex:representation ?feminineActivePastPerfectSecondPersonPlural ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q64005357, wd:Q51929049, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActiveThirdPersonSingularSubjunctiveForm .
    ?feminineActiveThirdPersonSingularSubjunctiveForm ontolex:representation ?feminineActiveThirdPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q51929074, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActiveThirdPersonDualSubjunctiveForm .
    ?feminineActiveThirdPersonDualSubjunctiveForm ontolex:representation ?feminineActiveThirdPersonDualSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q51929074, wd:Q110022, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePresentThirdPersonPluralSubjunctiveForm .
    ?feminineActivePresentThirdPersonPluralSubjunctiveForm ontolex:representation ?feminineActivePresentThirdPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePresentThirdPersonDualSubjunctiveForm .
    ?feminineActivePresentThirdPersonDualSubjunctiveForm ontolex:representation ?feminineActivePresentThirdPersonDualSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110022, wd:Q473746 .
  }
}

# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?masculinePassivePerformativeSecondPersonSingular
  ?masculinePassiveSecondPersonSingularSubjunctive
  ?masculinePassiveSecondPersonPluralSubjunctive
  ?masculinePassiveThirdPersonSingularSubjunctive
  ?masculinePassiveThirdPersonPluralSubjunctive
  ?masculinePassiveThirdPersonDualSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassivePerformativeSecondPersonSingularForm .
    ?masculinePassivePerformativeSecondPersonSingularForm ontolex:representation ?masculinePassivePerformativeSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q124351233, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveSecondPersonSingularSubjunctiveForm .
    ?masculinePassiveSecondPersonSingularSubjunctiveForm ontolex:representation ?masculinePassiveSecondPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929049, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveSecondPersonPluralSubjunctiveForm .
    ?masculinePassiveSecondPersonPluralSubjunctiveForm ontolex:representation ?masculinePassiveSecondPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929049, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveThirdPersonSingularSubjunctiveForm .
    ?masculinePassiveThirdPersonSingularSubjunctiveForm ontolex:representation ?masculinePassiveThirdPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929074, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveThirdPersonPluralSubjunctiveForm .
    ?masculinePassiveThirdPersonPluralSubjunctiveForm ontolex:representation ?masculinePassiveThirdPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929074, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculinePassiveThirdPersonDualSubjunctiveForm .
    ?masculinePassiveThirdPersonDualSubjunctiveForm ontolex:representation ?masculinePassiveThirdPersonDualSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1194697, wd:Q51929074, wd:Q110022, wd:Q473746 .
  }
}

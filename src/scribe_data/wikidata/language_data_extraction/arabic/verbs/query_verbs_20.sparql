# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?masculineActivePresentThirdPersonSingular
  ?masculineActiveSecondPersonPluralSubjunctive
  ?masculineActiveThirdPersonDualSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePresentThirdPersonSingularForm .
    ?masculineActivePresentThirdPersonSingularForm ontolex:representation ?masculineActivePresentThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActiveSecondPersonPluralSubjunctiveForm .
    ?masculineActiveSecondPersonPluralSubjunctiveForm ontolex:representation ?masculineActiveSecondPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q51929049, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActiveThirdPersonDualSubjunctiveForm .
    ?masculineActiveThirdPersonDualSubjunctiveForm ontolex:representation ?masculineActiveThirdPersonDualSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q51929074, wd:Q110022, wd:Q473746 .
  }
}

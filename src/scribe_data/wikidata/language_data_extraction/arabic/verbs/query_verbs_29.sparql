# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?feminineIndicativePassivePresentSecondPersonSingular
  ?feminineIndicativePassivePresentThirdPersonSingular
  ?feminineIndicativePassivePresentThirdPersonDual
  ?feminineIndicativeActivePresentSecondPersonSingular
  ?feminineIndicativeActivePresentThirdPersonPlural
  ?feminineIndicativeActivePresentThirdPersonDual

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineIndicativePassivePresentSecondPersonSingularForm .
    ?feminineIndicativePassivePresentSecondPersonSingularForm ontolex:representation ?feminineIndicativePassivePresentSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineIndicativePassivePresentThirdPersonSingularForm .
    ?feminineIndicativePassivePresentThirdPersonSingularForm ontolex:representation ?feminineIndicativePassivePresentThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929074, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineIndicativePassivePresentThirdPersonDualForm .
    ?feminineIndicativePassivePresentThirdPersonDualForm ontolex:representation ?feminineIndicativePassivePresentThirdPersonDual ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929074, wd:Q110022 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineIndicativeActivePresentSecondPersonSingularForm .
    ?feminineIndicativeActivePresentSecondPersonSingularForm ontolex:representation ?feminineIndicativeActivePresentSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q682111, wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineIndicativeActivePresentThirdPersonPluralForm .
    ?feminineIndicativeActivePresentThirdPersonPluralForm ontolex:representation ?feminineIndicativeActivePresentThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q682111, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineIndicativeActivePresentThirdPersonDualForm .
    ?feminineIndicativeActivePresentThirdPersonDualForm ontolex:representation ?feminineIndicativeActivePresentThirdPersonDual ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q682111, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110022 .
  }
}

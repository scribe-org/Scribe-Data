# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?masculineActivePresentSecondPersonSingular
  ?masculineActivePresentThirdPersonPlural
  ?masculineActivePresentThirdPersonDual
  ?masculineActivePastThirdPersonSingular
  ?masculineActivePresentSecondPersonPluralSubjunctive
  ?masculineActivePresentThirdPersonSingularSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePresentSecondPersonSingularForm .
    ?masculineActivePresentSecondPersonSingularForm ontolex:representation ?masculineActivePresentSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePresentThirdPersonPluralForm .
    ?masculineActivePresentThirdPersonPluralForm ontolex:representation ?masculineActivePresentThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePresentThirdPersonDualForm .
    ?masculineActivePresentThirdPersonDualForm ontolex:representation ?masculineActivePresentThirdPersonDual ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110022 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePastThirdPersonSingularForm .
    ?masculineActivePastThirdPersonSingularForm ontolex:representation ?masculineActivePastThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q1994301, wd:Q51929074, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePresentSecondPersonPluralSubjunctiveForm .
    ?masculineActivePresentSecondPersonPluralSubjunctiveForm ontolex:representation ?masculineActivePresentSecondPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineActivePresentThirdPersonSingularSubjunctiveForm .
    ?masculineActivePresentThirdPersonSingularSubjunctiveForm ontolex:representation ?masculineActivePresentThirdPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110786, wd:Q473746 .
  }
}

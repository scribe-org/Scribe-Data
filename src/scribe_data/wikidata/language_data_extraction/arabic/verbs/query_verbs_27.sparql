# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?feminineActivePresentSecondPersonPlural
  ?feminineActivePresentThirdPersonPlural
  ?feminineActivePastPerfectSecondPersonSingular
  ?feminineActiveSecondPersonSingularSubjunctive
  ?feminineActiveThirdPersonPluralSubjunctive
  ?feminineActivePresentSecondPersonSingularSubjunctive

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePresentSecondPersonPluralForm .
    ?feminineActivePresentSecondPersonPluralForm ontolex:representation ?feminineActivePresentSecondPersonPlural ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePresentThirdPersonPluralForm .
    ?feminineActivePresentThirdPersonPluralForm ontolex:representation ?feminineActivePresentThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePastPerfectSecondPersonSingularForm .
    ?feminineActivePastPerfectSecondPersonSingularForm ontolex:representation ?feminineActivePastPerfectSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q64005357, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActiveSecondPersonSingularSubjunctiveForm .
    ?feminineActiveSecondPersonSingularSubjunctiveForm ontolex:representation ?feminineActiveSecondPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q51929049, wd:Q110786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActiveThirdPersonPluralSubjunctiveForm .
    ?feminineActiveThirdPersonPluralSubjunctiveForm ontolex:representation ?feminineActiveThirdPersonPluralSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q51929074, wd:Q146786, wd:Q473746 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineActivePresentSecondPersonSingularSubjunctiveForm .
    ?feminineActivePresentSecondPersonSingularSubjunctiveForm ontolex:representation ?feminineActivePresentSecondPersonSingularSubjunctive ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q1317831, wd:Q192613, wd:Q51929049, wd:Q110786, wd:Q473746 .
  }
}

# tool: scribe-data
# All Arabic (Q13955) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?masculineIndicativePassivePresentSecondPersonSingular
  ?masculineIndicativePassivePresentThirdPersonPlural
  ?masculineIndicativeActivePresentThirdPersonSingular
  ?masculineIndicativeActivePresentThirdPersonDual

WHERE {
  ?lexeme dct:language wd:Q13955 ;
    wikibase:lexicalCategory wd:Q24905 ;
    wikibase:lemma ?verb ;
    schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineIndicativePassivePresentSecondPersonSingularForm .
    ?masculineIndicativePassivePresentSecondPersonSingularForm ontolex:representation ?masculineIndicativePassivePresentSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineIndicativePassivePresentThirdPersonPluralForm .
    ?masculineIndicativePassivePresentThirdPersonPluralForm ontolex:representation ?masculineIndicativePassivePresentThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q682111, wd:Q1194697, wd:Q192613, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineIndicativeActivePresentThirdPersonSingularForm .
    ?masculineIndicativeActivePresentThirdPersonSingularForm ontolex:representation ?masculineIndicativeActivePresentThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q682111, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?masculineIndicativeActivePresentThirdPersonDualForm .
    ?masculineIndicativeActivePresentThirdPersonDualForm ontolex:representation ?masculineIndicativeActivePresentThirdPersonDual ;
      wikibase:grammaticalFeature wd:Q499327, wd:Q682111, wd:Q1317831, wd:Q192613, wd:Q51929074, wd:Q110022 .
  }
}

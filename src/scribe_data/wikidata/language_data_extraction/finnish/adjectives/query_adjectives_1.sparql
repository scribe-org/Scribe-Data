# tool: scribe-data
# All finnish (Q1412) adjectives (Q34698) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
    (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?adjectives
    ?singularElative
  ?pluralComitative
  ?singularIllative
  ?singularNominativePositive
  ?superlative
  ?pluralInessive
  ?pluralEssive
  ?singularAbessive
  ?pluralTranslative
  ?singularPartitive
  ?singularAblative
  ?nominativePlural
  ?pluralElative
  ?singularNominative
  ?pluralIllative
  ?singularNominativeSuperlative
  ?comparative
  ?singularAllative
  ?singularGenitivePositive
